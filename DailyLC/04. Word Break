class Solution {
    public boolean wordBreak(String s, List<String> wordDict) {
        int len=s.length();
        if(s==null || len==0){
            return true;
        }
        boolean[] dp= new boolean[len];
        
        for(int i=0;i<len;i++){
            for(int j=0;j<=i;j++){
                String currSub=s.substring(j,i+1);
                
                if(wordDict.contains(currSub) && (j==0 || dp[j-1]==true)){
                    dp[i]=true;
                    break;
                }
            }
        }
        
        return dp[len-1];
    }
}
